            <!--android:layout_centerVertical="true"-->
            <!--android:text="Status"-->
            <!--android:paddingRight="5dp"-->
            <!--/>-->

        <!--</LinearLayout>-->
    </RelativeLayout>

</android.support.v7.widget.CardView>

