<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nomination_card_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:foreground="?android:attr/selectableItemBackground"
    card_view:cardCornerRadius="4dp"
    android:layout_margin="3dp">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <!--android:padding="5dip">-->

        <LinearLayout
            android:id="@+id/member_thumbnail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="3dip"
            android:layout_alignParentLeft="true"
            android:layout_marginRight="5dip">

            <!--<com.android.volley.toolbox.NetworkImageView-->
            <!--android:layout_width="75dp"-->
            <!--android:layout_height="75dp"-->
            <!--android:id="@+id/nomination_pic"/>-->
            <app.psiteportal.com.utils.CircleImageView
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:id="@+id/member_pic"/>

        </LinearLayout>
        <!--<LinearLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:orientation="vertical">-->
        <!--<LinearLayout-->
            <!--android:id="@+id/layout2"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_toRightOf="@id/member_thumbnail"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:orientation="vertical">-->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/member_thumbnail"
            android:layout_marginTop="5dp"
            android:textStyle="bold"
            android:text="Name"
            android:id="@+id/member_name"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/member_name"
            android:text="Institution"
            android:layout_toRightOf="@+id/member_thumbnail"
            android:id="@+id/member_institution"/>
            <!--android:layout_toLeftOf="@+id/member_status"-->


<!--</LinearLayout>-->
        <!--<android.support.v7.widget.AppCompatRadioButton-->
        <!--android:id="@+id/radioButton"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_centerVertical="true"-->
        <!--/>-->

        <TextView
            android:id="@+id/member_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/member_institution"
            android:layout_toRightOf="@+id/member_thumbnail"
            android:text="Status"
            />
        <!--<TextView-->
            <!--android:id="@+id/member_status"-->
            <!--android:layout_width="wrap_content"-->
